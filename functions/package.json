    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "express": "^4.16.4",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.1",
    "firebase-functions-test": "^0.1.6",
    "nuxt": "^2.7.0"
